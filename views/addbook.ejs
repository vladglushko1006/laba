<div class="container">
	<form method="POST" class="form-group" action="/submit">
		<div class="form-group">
			<input 
				type="text" 
				class="form-control" 
				name="name" 
				placeholder="Enter name"
			>
		</div>
		<div class="form-group">
			<input 
				type="text" 
				name="content" 
				class="form-control" 
				placeholder="Enter content"
			>
		<div class="form-group">
			<input 
				type="submit"
				class="btn btn-success form-control" 
				value="Create"
			>
		</div>
	</form>
</div>
